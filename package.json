{
  "name": "claude-tabbed-terminal",
  "displayName": "Claude Tabbed Terminal",
  "description": "A VS Code extension with tabbed terminals running Claude via node-pty and xterm.js",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:claude-tabbed-terminal.openTerminal"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "claude-tabbed-terminal.openTerminal",
        "title": "Open Claude Tabbed Terminal"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@vscode/test-electron": "^2.2.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "node-pty": "^0.10.1"
  }
}
